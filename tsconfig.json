{
  "compilerOptions": {
    "target": "es2020",
    // "lib": [
    //   "es6"
    // ],
    "module": "commonjs",
    "moduleResolution": "node",
    "rootDir": "",
    "removeComments": false,
    "sourceMap": true,
    "noEmitOnError": true,
    "resolveJsonModule": true,
    "allowJs": true,
    "declaration": true,
    "outDir": "dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": true,
    "skipLibCheck": true
  },
  "exclude":[
    "./node_modules",
    "./dist",
  ]
}
